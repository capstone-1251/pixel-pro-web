{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"area": "footer"
		}
	],
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#187F8E",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#63B1BC",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#14C7A8",
					"name": "Tertiary"
				},
				{
					"slug": "accent",
					"color": "#6BCABA",
					"name": "Accent"
				},
				{
					"slug": "quaternary",
					"color": "#414142",
					"name": "Quaternary"
				},
				{
					"slug": "gray",
					"color": "#898D8D",
					"name": "Gray"
				},
				{
					"slug": "button",
					"color": "#F1911D",
					"name": "Button"
				},
				{
					"slug": "hover",
					"color": "#EFBE7D",
					"name": "Hover"
				},
				{
					"slug": "background",
					"color": "#EFE1C7",
					"name": "Background"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "text-link",
					"color": "#3D3D3E",
					"name": "Text Link"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Nunito\", sans-serif",
					"slug": "nunito",
					"name": "Nunito",
					"fontFace": [
						{
							"fontFamily": "\"Nunito\", sans-serif",
							"fontWeight": "300",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Nunito-Light.woff2"
							]
						},
						{
							"fontFamily": "\"Nunito\", sans-serif",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Nunito-Bold.woff2"
							]
						},
						{
							"fontFamily": "\"Nunito\", sans-serif",
							"fontWeight": "800",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Nunito-ExtraBold.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Clarendon\", serif",
					"slug": "clarendon",
					"name": "Clarendon",
					"fontFace": [
						{
							"fontFamily": "\"Clarendon\", serif",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/URWClarendonTOT-Med.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Heading 1",
					"slug": "heading-1",
					"size": "4rem"
				},
				{
					"name": "Heading 2",
					"slug": "heading-2",
					"size": "3rem"
				},
				{
					"name": "Heading 3",
					"slug": "heading-3",
					"size": "2.5rem"
				},
				{
					"name": "Heading 4",
					"slug": "heading-4",
					"size": "2rem"
				},
				{
					"name": "Body Large",
					"slug": "body-large",
					"size": "1.125rem"
				},
				{
					"name": "Body Small",
					"slug": "body-small",
					"size": "1rem"
				},
				{
					"name": "Button Link",
					"slug": "button-link",
					"size": "1.25rem"
				},
				{
					"name": "Heading-1 Mobile",
					"slug": "heading-1-mobile",
					"size": "2rem"
				},
				{
					"name": "Heading-2 Mobile",
					"slug": "heading-2-mobile",
					"size": "1.75rem"
				},
				{
					"name": "Heading-3 Mobile",
					"slug": "heading-3-mobile",
					"size": "1.375rem"
				},
				{
					"name": "Button Link Mobile",
					"slug": "button-link-mobile",
					"size": "1rem"
				}
			],
			"lineHeight": false,
			"customFontSize": false
		},
		"spacing": {
			"blockGap": true,
			"customSpacingSize": true,
			"spacingSizes": [
				{
					"slug": "8",
					"size": "0.5rem",
					"name": "8px"
				},
				{
					"slug": "16",
					"size": "1rem",
					"name": "16px"
				},
				{
					"slug": "24",
					"size": "1.5rem",
					"name": "24px"
				},
				{
					"slug": "32",
					"size": "2rem",
					"name": "32px"
				},
				{
					"slug": "42",
					"size": "2.625rem",
					"name": "42px"
				},
				{
					"slug": "56",
					"size": "3.5rem",
					"name": "56px"
				},
				{
					"slug": "72",
					"size": "4.5rem",
					"name": "72px"
				},
				{
					"slug": "80",
					"size": "5rem",
					"name": "80px"
				},
				{
					"slug": "96",
					"size": "6rem",
					"name": "96px"
				},
				{
					"slug": "120",
					"size": "7.5rem",
					"name": "120px"
				},
				{
					"slug": "hero-padding",
					"size": "3.75rem",
					"name": "Hero Padding (Top/Bottom)"
				},
				{
					"slug": "header-to-content",
					"size": "2.25rem",
					"name": "Header to Content"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"layout": {
			"contentSize": "1432px",
			"wideSize": "1432px"
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"spacing": {
			"padding": {
				"top": "1rem",
				"right": "1rem",
				"bottom": "1rem",
				"left": "1rem"
			}
		},
		"typography": {
			"lineHeight": "1.5",
			"fontFamily": "var(--wp--preset--font-family--nunito)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--clarendon)",
					"fontSize": "var(--wp--preset--font-size--heading-1)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--clarendon)",
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--clarendon)",
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--clarendon)",
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--button)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--nunito)",
					"fontWeight": "700",
					"fontSize": "16px"
				},
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--nunito)",
					"fontWeight": "800",
					"fontSize": "var(--wp--preset--font-size--)"
				},
				"border": {
					"radius": "3.125rem"
				},
				"color": {
					"background": "var(--wp--preset--color--button)",
					"text": "var(--wp--preset--color--white)"
				},
				"spacing": {
					"padding": {
						"top": "1em",
						"bottom": "1em",
						"left": "5em",
						"right": "5em"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--nunito)",
					"fontSize": "var(--wp--preset--font-size--body-small)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "3.75rem",
						"bottom": "3.75rem"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "2rem"
				}
			}
		}
	}
}